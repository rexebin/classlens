{
  "name": "classlens",
  "displayName": "ClassLens",
  "description":
    "Class lens showing relationships with its interfaces and parent class.",
  "version": "0.0.3",
  "publisher": "rexebin",
  "engines": {
    "vscode": "^1.22.0"
  },
  "icon": "classlens.png",
  "repository": {
    "url": "https://github.com/rexebin/classlens"
  },
  "categories": ["Languages", "Other"],
  "activationEvents": ["onLanguage:typescript"],
  "contributes": {
    "configuration": {
      "title": "Side by side",
      "properties": {
        "classLens.openSideBySide": {
          "type": "boolean",
          "default": true,
          "description": "Open files side by side to the Typescript file."
        }
      }
    }
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
